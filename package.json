    "antd": "^5.4.5",
    "classnames": "^2.3.2",
    "echarts": "^5.4.2",
    "echarts-for-react": "^3.0.2",
    "jotai": "^2.0.4",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
