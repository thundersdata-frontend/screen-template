    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.8",
    "standard-version": "^9.5.0",
    "stylelint": "^15.6.0",
    "typescript": "^5.0.4"
  },
  "resolutions": {
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.0"
