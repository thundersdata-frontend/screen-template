{
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "insertPragma": false,
  "tabWidth": 2,
  "useTabs": false,
  "proseWrap": "never",
  "overrides": [
    {
      "files": [
        ".prettierrc",
        "*.json"
      ],
      "options": {
        "parser": "json"
      }
    }
  ]
}